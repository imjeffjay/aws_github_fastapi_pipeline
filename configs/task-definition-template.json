{
    "family": "FastAPITask",
    "containerDefinitions": [
      {
        "name": "fastapi-app",
        "image": "<your-account-id>.dkr.ecr.<region>.amazonaws.com/fastapi-app:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000
          }
        ]
      }
    ]
  }
  